<!DOCTYPE html>
<html>
<head>
  <title>Products</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag    '//maxcdn.bootstrapcdn.com/bootswatch/3.3.1/yeti/bootstrap.min.css' %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= javascript_include_tag '//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js' %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/header'%>
<% if alert %>
    <div id="page-message" class="page-message alert">
      <%= alert %>
    </div>
<% end %>

<% if notice %>
    <div id="page-message" class="page-message notice">
      <%= notice %>
    </div>
<% end %>

<div class="container">
  <div class="row">
    <div class="col-md-2">
      <%= render 'layouts/menu' %>
    </div>
    <div class="col-md-10">
        <%= yield %>
    </div>
  </div>
</div>


</body>
</html>
